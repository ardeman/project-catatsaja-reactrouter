{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "react-router.config.ts",
    "app/root.tsx",
    "app/entry.{client,server}.{js,jsx,ts,tsx}",
    "app/routes/**/*.{js,ts,tsx}",
    "app/routes.ts",
    "app/catch-all.tsx",
    "server.{js,ts}"
  ],
  "project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"],
  "ignore": ["app/components/ui/*.{js,ts,tsx}", "app/localization/resource.ts"],
  "ignoreDependencies": ["eslint-import-resolver-typescript"]
}
